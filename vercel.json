{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets|imagens|src|.*\\..*).*)",
      "destination": "/dashboard.html"
    }
  ]
}