{
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/imagens/(.*)",
      "destination": "/imagens/$1"
    },
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)\\.(.*)",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/dashboard.html"
    }
  ]
}